{
  "name": "@koex/router",
  "version": "0.0.16",
  "description": "router for koa extend",
  "main": "lib/index.js",
  "repository": "https://github.com/koexjs/router",
  "author": "Zero",
  "license": "MIT",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf -rf lib",
    "test": "jest -w 1",
    "coverage": "codecov",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/jest": "^24.0.19",
    "@types/koa": "^2.0.49",
    "@types/koa-compose": "^3.2.4",
    "@zcorky/delay": "^1.0.1",
    "global": "^4.3.2",
    "husky": "^1.2.0",
    "jest": "^24.9.0",
    "koa": "^2.8.1",
    "rimraf": "^2.6.2",
    "should": "^13.2.3",
    "source-map-support": "^0.5.9",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-jsdoc-rules": "^0.1.2",
    "typescript": "^3.6.3"
  },
  "files": [
    "lib/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "jest": {
    "roots": [
      "__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.(spec|test)\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 80,
        "statements": -10
      }
    },
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "koa-compose": "^4.1.0",
    "path-to-regexp": "^3.1.0"
  }
}
