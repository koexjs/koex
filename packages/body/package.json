{
  "name": "@koex/body",
  "version": "0.13.28",
  "description": "body parser for koex",
  "main": "lib/index.js",
  "repository": "https://github.com/koexjs/router",
  "author": "Zero",
  "license": "MIT",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "rm -rf lib",
    "test": "jest -w 1",
    "coverage": "codecov"
  },
  "devDependencies": {
    "@koex/onerror": "^0.0.3",
    "@types/co-body": "^5.1.0",
    "@types/debug": "^0.0.31",
    "@types/jest": "^24.0.19",
    "@types/koa": "^2.0.49",
    "@types/koa-compose": "^3.2.4",
    "@zcorky/delay": "^1.0.1",
    "global": "^4.3.2",
    "jest": "^24.9.0",
    "koa": "^2.8.1",
    "rimraf": "^2.6.2",
    "should": "^13.2.3",
    "source-map-support": "^0.5.9",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-jsdoc-rules": "^0.1.2",
    "typescript": "^3.6.3"
  },
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "jest": {
    "roots": [
      "__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.(spec|test)\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 80,
        "statements": -10
      }
    },
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@koex/core": "^0.13.28",
    "@types/formidable": "1.0.31",
    "@zcorky/is": "^1.0.6",
    "co-body": "^6.0.0",
    "formidable": "1.2.2"
  }
}
