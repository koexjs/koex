{
  "name": "@koex/static",
  "version": "0.13.36",
  "description": "static cache for koa extend",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "repository": "https://github.com/koexjs/static",
  "author": "Zero",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "rm -rf lib",
    "test": "jest -w 1",
    "coverage": "codecov"
  },
  "devDependencies": {
    "@zcorky/koa-router": "^0.0.2",
    "@zcorky/lru": "^1.0.3"
  },
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "jest": {
    "roots": [
      "__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.(spec|test)\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 90,
        "lines": 80,
        "statements": -20
      }
    },
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "compressible": "^2.0.15",
    "koa-compose": "^4.1.0",
    "koa-range": "^0.3.0",
    "mime-types": "^2.1.21",
    "mz": "^2.7.0",
    "stream-wormhole": "^1.1.0"
  }
}
