{
  "name": "@koex/static",
  "version": "0.0.1",
  "description": "static cache for koa extend",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "repository": "https://github.com/koexjs/static",
  "author": "Zero",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf -rf lib",
    "test": "NODE_ENV=test nyc mocha test/*.spec.ts test/**/*.spec.ts --exit --timeout 10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/compressible": "^2.0.0",
    "@types/debug": "^0.0.31",
    "@types/koa": "^2.0.47",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^5.2.5",
    "@types/ms": "^0.7.30",
    "@types/mz": "^0.0.32",
    "@types/node": "^10.9.4",
    "@types/sinon": "^5.0.2",
    "@zcorky/delay": "^1.0.1",
    "@zcorky/koa-router": "^0.0.2",
    "@zcorky/lru": "^1.0.2",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.0.2",
    "debug": "^4.1.0",
    "global": "^4.3.2",
    "koa": "^2.6.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "should": "^13.2.3",
    "sinon": "^6.3.4",
    "source-map-support": "^0.5.9",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-jsdoc-rules": "^0.1.2",
    "typescript": "^3.1.1"
  },
  "files": [
    "lib/"
  ],
  "nyc": {
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "compressible": "^2.0.15",
    "mime-types": "^2.1.21",
    "mz": "^2.7.0"
  }
}
