{
  "name": "@koex/static",
  "version": "0.0.19",
  "description": "static cache for koa extend",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "repository": "https://github.com/koexjs/static",
  "author": "Zero",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf -rf lib",
    "test": "jest -w 1",
    "coverage": "codecov",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/compressible": "^2.0.0",
    "@types/debug": "^0.0.31",
    "@types/jest": "^24.0.19",
    "@types/koa": "^2.0.49",
    "@types/mime-types": "^2.1.0",
    "@types/ms": "^0.7.30",
    "@types/mz": "^0.0.32",
    "@types/node": "^12.7.5",
    "@zcorky/delay": "^1.0.1",
    "@zcorky/koa-router": "^0.0.2",
    "@zcorky/lru": "^1.0.2",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.0.2",
    "debug": "^4.1.0",
    "global": "^4.3.2",
    "jest": "^24.9.0",
    "koa": "^2.8.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "jest": {
    "roots": [
      "__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.(spec|test)\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 80,
        "statements": -10
      }
    },
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "compressible": "^2.0.15",
    "mime-types": "^2.1.21",
    "mz": "^2.7.0"
  }
}
